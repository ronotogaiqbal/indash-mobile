			"name": "Info & List",
			"description": "Endpoint untuk mendapatkan informasi API dan daftar data yang tersedia",
			"item": [
				{
					"name": "Get API Info",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}{{apiPath}}",
							"host": ["{{baseUrl}}{{apiPath}}"]
						},
						"description": "Mendapatkan informasi API dan daftar endpoint yang tersedia."
					},
					"response": [
						{
							"name": "Success",
							"originalRequest": {
								"method": "GET",
								"url": {
									"raw": "{{baseUrl}}{{apiPath}}"
								}
							},
							"status": "OK",
							"code": 200,
							"body": "{\n  \"status\": \"success\",\n  \"message\": \"INDASH GeoJSON API\",\n  \"version\": \"1.0.0\",\n  \"endpoints\": {\n    \"getKecamatan\": \"Get all kecamatan boundaries\",\n    \"getKecamatanById\": \"Get kecamatan by KDCPUM (id parameter)\",\n    \"getDesa\": \"Get desa boundaries by kecamatan (kdcpum parameter)\",\n    \"getLahan\": \"Get lahan boundaries by kecamatan (kdcpum parameter)\",\n    \"getByBounds\": \"Get kecamatan within bounds (minLat, minLng, maxLat, maxLng)\",\n    \"getByLocation\": \"Get kecamatan & desa containing GPS point (lat, lng) - for mobile\",\n    \"listKecamatan\": \"List all kecamatan IDs\",\n    \"listDesa\": \"List all available desa files\",\n    \"listLahan\": \"List all available lahan files\"\n  }\n}"
						}
					]
				},
